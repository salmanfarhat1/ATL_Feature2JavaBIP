<?xml version="1.0" encoding="UTF-8"?>
<Feature_model
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns="https://fr.wikipedia.org/wiki/Feature_model"
    xsi:schemaLocation="https://fr.wikipedia.org/wiki/Feature_model ../MetaModels/Feature_MM.ecore">
  <root name="Mobile_Phone">
    <SubFeatures
        xsi:type="Alternative"
        name="GPS"/>
    <SubFeatures
        xsi:type="Alternative"
        name="Media">
      <SubFeatures
          xsi:type="Exclusive"
          name="Camera"/>
      <SubFeatures
          xsi:type="Exclusive"
          name="MP3"/>
    </SubFeatures>
    <SubFeatures
        xsi:type="Alternative"
        name="Screen">
      <SubFeatures
          xsi:type="Exclusive"
          name="Basic"/>
      <SubFeatures
          xsi:type="Exclusive"
          name="Colour"/>
      <SubFeatures
          xsi:type="Exclusive"
          name="High_Resolution"/>
    </SubFeatures>
    <SubFeatures
        xsi:type="Alternative"
        name="Calls"/>
  </root>
  <constraints
      xsi:type="Implies"
      F_From="//@root/@SubFeatures.1/@SubFeatures.0"
      F_to="//@root/@SubFeatures.2/@SubFeatures.2"/>
</Feature_model>
